{
  "name": "ella-bootstrap-theme-kit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch-main-scss": "node-sass -w scss/main -o public/stylesheets/main",
    "build-ella-scss-template": "node-sass -o public/stylesheets/my_scss_template scss/my_scss_template",
    "build-bootstrap": "node-sass -o public/stylesheets/bootstrap scss/bootstrap-4/scss/bootstrap.scss",
    "run-all": "npm run build-bootstrap && npm run build-ella-scss-template && npm run watch-main-scss"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "node-sass": "^5.0.0"
  }
}
